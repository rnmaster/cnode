{
  "presets": [
    "react-native"
  ],
  "plugins": [
    ["babel-plugin-module-alias", [
      { "src": "./app", "expose": "app" },
      { "src": "./app/component", "expose": "component" },
      { "src": "./app/asset", "expose": "asset" },
    ]]
    ]],
    ["antd", {
      "libraryName":"antd-mobile",
    }],
    "transform-decorators-legacy"
  ]
}